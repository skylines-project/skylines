{
  "extends": [
    "config:js-app",
    ":dependencyDashboard",
    ":maintainLockFilesWeekly",
    ":semanticCommitsDisabled",
    ":automergeLinters",
    ":automergeTesters"
  ],
  "postUpdateOptions": [
    "yarnDedupeFewer"
  ],
  "packageRules": [
    {
      "matchCategories": [
        "python"
      ],
      "enabled": false
    },
    {
      "matchPackageNames": [
        "ember-cli",
        "ember-data",
        "ember-source"
      ],
      "separateMinorPatch": true
    }
  ]
}
